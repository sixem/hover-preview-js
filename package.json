{
  "name": "hover-preview-js",
  "version": "2.0.0",
  "description": "Adds hoverable video and image previews to elements in Vanilla JS.",
  "main": "./dist/hover-preview.min.js",
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack --config ./webpack.config.js && cross-env NODE_ENV=production webpack --config ./webpack.config.js",
