    "cross-env": "^7.0.2",
    "eslint": "^7.6.0",
    "moment": "^2.27.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "browserslist": "> 0.25%, not dead",
